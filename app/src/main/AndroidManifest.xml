<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="us.icter.dirigentes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="us.icter.activities.ConfigActivity"
            android:noHistory="true" />
        <activity android:name="us.icter.activities.QRActivity" />
        <activity
            android:name="us.icter.activities.ImageActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="us.icter.activities.VideoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>